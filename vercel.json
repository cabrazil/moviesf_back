{
  "functions": {
    "api/server.ts": {
      "maxDuration": 30
    },
    "src/server.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/server.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/src/server.ts"
    }
  ]
}